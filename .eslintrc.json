{
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "globals": {
    "jQuery": "readonly",
    "$": "readonly",
    "window": "readonly",
    "require": "readonly",
    "process": "readonly"
  },
  "rules": {
    "no-undef": "error",
    "eqeqeq": ["error", "always"]
  }
}